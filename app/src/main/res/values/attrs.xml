<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--卫星菜单栏-->
    <attr name="position">
        <enum name="left_top" value="0"/>
        <enum name="left_bottom" value="1"/>
        <enum name="right_top" value="2"/>
        <enum name="right_bottom" value="3"/>
        <enum name="center_top" value="4"/>
        <enum name="center_bottom" value="5"/>
        <enum name="center" value="6"/>
        <enum name="center_left" value="7"/>
        <enum name="center_right" value="8"/>
    </attr>
    <attr name="radius" format="dimension"></attr>
    <declare-styleable name="SolarSystem">
        <attr name="position"/>
        <attr name="radius"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="borderWidth" format="dimension"/>
        <attr name="borderColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CircleProgress">
        <!--圆的半径-->
        <attr name="radius"/>
        <!--总进度条颜色-->
        <attr name="totalColor" format="color"/>
        <!--进度条颜色-->
        <attr name="proColor" format="color"/>
        <!--进度条宽度-->
        <attr name="proWidth" format="dimension"/>
        <!--字体内容-->
        <attr name="text" format="string"/>
        <!--字体颜色-->
        <attr name="textcolor" format="color"/>
        <!--字体颜色-->
        <attr name="textsize" format="dimension"/>
        <!--字体背景-->
        <attr name="textBackground" format="color|reference"/>
    </declare-styleable>

</resources>