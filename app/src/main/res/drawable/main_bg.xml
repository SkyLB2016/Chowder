<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/wh_10" />
            <gradient android:endColor="@color/color_66" android:startColor="@color/main_color" />
            <stroke android:width="@dimen/wh_2" android:color="@color/green" android:dashGap="@dimen/wh_2" android:dashWidth="@dimen/wh_8" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/wh_10" />
            <solid android:color="@color/main_color" />
            <stroke android:width="@dimen/wh_2" android:color="@color/green" android:dashGap="@dimen/wh_2" android:dashWidth="@dimen/wh_8" />
        </shape>
    </item>
</selector>